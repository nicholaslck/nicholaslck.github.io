		"prettier-plugin-tailwindcss": "^0.5.9",
		"rehype": "^13.0.1",
		"rehype-class-names": "^1.0.14",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"dateformat": "^5.0.3"
	}
